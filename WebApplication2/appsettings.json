{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Csp": {
    "ReportOnly": false,
    "Policies": {
      "default-src": [ "'self'" ],
      "script-src": [ "'self'" ],
      "style-src": [ "'self'" ],
      "font-src": [ "'self'" ],
      "img-src": [ "'self'" ],
      "connect-src": [ "*.google-analytics.com" ],
      "frame-ancestors": [ "'self'" ]
    }
  }
}

